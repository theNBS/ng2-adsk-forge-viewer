{
  "name": "ng2-adsk-forge-viewer",
  "version": "0.2.1",
  "description": "Angular wrapper for Autodesk Forge Viewer",
  "main": "./bundles/ng2-adsk-forge-viewer.umd.js",
  "module": "./ng2-adsk-forge-viewer.es5.js",
  "es2015": "./ng2-adsk-forge-viewer.js",
  "typings": "./ng2-adsk-forge-viewer.d.ts",
  "author": "NBS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theNBS/ng2-adsk-forge-viewer.git"
  },
  "keywords": [
    "autodesk",
    "forge",
    "angular"
  ],
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "build-demo": "tsc -p src/demo/",
    "build-demo:watch": "tsc -p src/demo/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build-demo",
    "start": "concurrently \"npm run build-demo:watch\" \"npm run serve\"",
    "build-test": "tsc -p src/lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p src/lib/tsconfig.spec.json -w",
    "pretest": "npm run build-test",
    "test": "concurrently \"npm run build-test:watch\" \"karma start karma.conf.js\"",
    "coverage": "karma start karma-coverage.conf.js --single-run",
    "pretest:once": "npm run build-test",
    "test:once": "karma start karma.conf.js --single-run",
    "preintegration": "npm run build && cd integration && npm install",
    "integration": "npm run integration:jit",
    "integration:jit": "cd integration && npm run e2e",
    "integration:aot": "cd integration && npm run e2e:aot",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "release": "standard-version"
  },
  "dependencies": {
    "@types/three": "^0.91.0",
    "@types/forge-viewer": "^6.3.1"
  },
  "peerDependencies": {
    "@angular/core": ">=5.0.0 <8.0.0"
  },
  "devDependencies": {
    "@angular/common": "~5.1.3",
    "@angular/compiler": "~5.1.3",
    "@angular/compiler-cli": "~5.1.3",
    "@angular/core": "~5.1.3",
    "@angular/platform-browser": "~5.1.3",
    "@angular/platform-browser-dynamic": "~5.1.3",
    "@types/jasmine": "^2.7.0",
    "@types/node": "^6.0.46",
    "camelcase": "^4.0.0",
    "concurrently": "3.4.0",
    "core-js": "^2.4.1",
    "glob": "^7.1.1",
    "jasmine-core": "^2.7.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-coverage": "^0.1.4",
    "lite-server": "^2.4.0",
    "ng2-mock-component": "^0.1.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.42.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.2",
    "standard-version": "^4.0.0",
    "systemjs": "^0.19.40",
    "ts-mockito": "^2.3.0",
    "tslint": "^5.9.1",
    "tslint-config-nbs": "^5.4.3",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.4"
  }
}
